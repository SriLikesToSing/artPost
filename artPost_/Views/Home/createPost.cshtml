@{
    ViewData["Title"] = "Create Post";
}

<p>create post!</p>

<div class="text-center">
    <h1 class="display-4">Create a New Post</h1>
    <form method="POST" enctype="multipart/form-data">
        <input type="text" id="title" name="Title" placeholder="Post Title">
        <input type="textarea" id="description" name="Description" placeholder="Post Description">
        <input type="file" id="postImage" name="Image" accept="image/*"/>
        <br />
        <input onclick="/Home/Profile"type="submit" value="Submit" />
    </form>
</div>
